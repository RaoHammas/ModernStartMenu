<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundColor">#252526</Color>
    <Color x:Key="ForegroundColor">#5E5E5E</Color>
    <Color x:Key="MiddleColor">#171718</Color>
    <Color x:Key="TopSectionColor">#353536</Color>


    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="ForegroundBrush" Color="{StaticResource ForegroundColor}" />
    <SolidColorBrush x:Key="MiddleBrush" Color="{StaticResource MiddleColor}" />
    <SolidColorBrush x:Key="ToSectionBrush" Color="{StaticResource TopSectionColor}" />

</ResourceDictionary>